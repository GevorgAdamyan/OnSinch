{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended"
  ],
  "overrides": [
  ],
  "plugins": [
    "cypress"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-multi-spaces": "error",
    "no-multiple-empty-lines": "error",
    "no-trailing-spaces": "error",
    "no-whitespace-before-property": "error",
    "operator-linebreak": "error",
    "quotes": [
      "error",
      "single"
    ],
    "semi": "error"
  }
}
